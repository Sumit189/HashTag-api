{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
        {
          "src": "/trendingHashtags",
          "dest": "app.py"
        },
        {
          "src": "/(.*)",
          "dest": "/"
        }
    ],
    "env": {
      "FLASK_ENV": "production"
    }
  }